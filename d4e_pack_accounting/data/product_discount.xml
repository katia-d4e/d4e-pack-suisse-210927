<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <record id="product_category_discount" model="product.category">
        <field name="parent_id" ref="product.product_category_1" />
        <field name="name">Discount</field>
    </record>

    <record id="product_discount" model="product.product">
        <field name="name">Discount</field>
        <field name="standard_price">0.00</field>
        <field name="list_price">0.00</field>
        <field name="weight">0.00</field>
        <field name="type">consu</field>
        <field name="categ_id" ref="product_category_discount" />
        <field name="uom_id" ref="uom.product_uom_unit" />
        <field name="uom_po_id" ref="uom.product_uom_unit" />
        <field name="default_code">REM</field>
        <field name="sale_ok">False</field>
        <field name="purchase_ok">False</field>
    </record>

    <record id="discount_ttc" model="product.product">
        <field name="name">Discount TTC</field>
        <field name="standard_price">0.00</field>
        <field name="list_price">0.00</field>
        <field name="weight">0.00</field>
        <field name="type">consu</field>
        <field name="categ_id" ref="product_category_discount" />
        <field name="uom_id" ref="uom.product_uom_unit" />
        <field name="uom_po_id" ref="uom.product_uom_unit" />
        <field name="property_account_income_id" ref="uom.product_uom_unit" />
        <field name="taxes_id" eval="[(4, ref('l10n_ch.1_vat_77_incl'))]" />
        <field name="default_code">REM_TTC</field>
        <field name="sale_ok">False</field>
        <field name="purchase_ok">False</field>
    </record>

    <record id="discount_ht" model="product.product">
        <field name="name">Discount HT</field>
        <field name="standard_price">0.00</field>
        <field name="list_price">0.00</field>
        <field name="weight">0.00</field>
        <field name="type">consu</field>
        <field name="categ_id" ref="product_category_discount" />
        <field name="uom_id" ref="uom.product_uom_unit" />
        <field name="uom_po_id" ref="uom.product_uom_unit" />
        <field name="taxes_id" eval="[(4, ref('l10n_ch.1_vat_77'))]" />
        <field name="default_code">REM_HT</field>
        <field name="sale_ok">False</field>
        <field name="purchase_ok">False</field>
    </record>
</odoo>
